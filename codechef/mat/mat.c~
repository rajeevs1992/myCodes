#include<stdio.h>
int main()
{
    unsigned long longint n,m,t;
    scanf("%d",&t);
    while(t--)
    {
    scanf("%d",&n);
    scanf("%d",&m);
    if(m==1)
        printf("%d\n",n);
    else if(n==1)
        printf("%d\n",m);
    else if(m==n)
        printf("%d\n",m);
    else
        printf("2\n");
    }
        
}
